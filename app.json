{
  "expo": {
    "name": "TJC-app",
    "slug": "TJC-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tjc.torneo",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "instagram",
          "whatsapp"
        ]
      }
    },
    "android": {
      "package": "com.tjc.torneo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "instagram"
          }
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "whatsapp"
          }
        }
      ]
    },
    "web": {
      "favicon": "./assets/icon.png",
      "themeColor": "#000000"
    },
    "plugins": [
      "expo-web-browser"
    ]
  }
}
